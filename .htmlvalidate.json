{
  "extends": [
    "html-validate:recommended",
    "html-validate:prettier",
    "html-validate:document"
  ],
  "rules": {
    "no-inline-style": "off",
    "require-sri": "off",
    "text-content": "off",
    "heading-level": "off",
    "unique-landmark": "off",
    "no-unknown-elements": "error"
  }
}
